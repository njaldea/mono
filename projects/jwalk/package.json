{
    "name": "@nil-/jwalk",
    "description": "a library that traverses json like a tree of customizable nodes",
    "version": "0.0.4",
    "author": {
        "email": "njaldea@gmail.com",
        "name": "Neil Aldea"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/njaldea/mono.git"
    },
    "license": "ISC",
    "scripts": {
        "build": "vite build",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "dev": "vite dev",
        "gen:package": "svelte-package -o package",
        "gen:meta": "npx @nil-/postpackage ./package",
        "package": "pnpm run gen:package && pnpm run gen:meta",
        "sync": "svelte-kit sync",
        "test:unit": "vitest",
        "lint-": "eslint . --ignore-path ../../.eslintignore"
    },
    "publishConfig": {
        "directory": "package",
        "linkDirectory": true
    },
    "devDependencies": {
        "@nil-/doc": "workspace:^1.0.0",
        "@nil-/doc-kit": "workspace:^1.0.0",
        "@nil-/postpackage": "workspace:^0.0.4",
        "@sveltejs/adapter-vercel": "^2.4.3",
        "@sveltejs/kit": "^1.21.0",
        "@sveltejs/package": "^2.1.0",
        "@types/three": "^0.152.1",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-svelte": "^2.32.2",
        "jsonpointer": "^5.0.1",
        "mdsvex": "^0.11.0",
        "svelte": "^4.0.3",
        "svelte-check": "^3.4.4",
        "three": "^0.153.0",
        "tslib": "^2.6.0",
        "typescript": "^5.1.6",
        "vite": "^4.3.9",
        "vitest": "^0.26.3"
    },
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./index.d.ts",
            "default": "./index.js"
        },
        "./helpers/js": {
            "types": "./helpers/js.d.ts",
            "default": "./helpers/js.js"
        },
        "./helpers/debug": {
            "types": "./helpers/debug.d.ts",
            "default": "./helpers/debug.js"
        }
    },
    "keywords": [
        "JSON",
        "tree",
        "schema"
    ]
}
