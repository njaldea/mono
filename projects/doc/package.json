        "svelte-check": "^2.9.2",
        "svelte-markdown": "^0.2.3",
        "svelte-preprocess": "^4.10.7",
        "tslib": "^2.4.1",
        "typescript": "^4.8.4",
        "vite": "^3.2.3"
    },
    "type": "module"
