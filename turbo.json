{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["sync", "^package"]
        },
        "check": {
            "dependsOn": ["sync", "^package"]
        },
        "dev": {
            "dependsOn": ["sync", "^package"]
        },
        "package": {
            "dependsOn": ["sync"],
            "outputs": ["./package/**"]
        },
        "sync": {
            "outputs": ["./svelte-kit/**"]
        }
    }
}
