{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^package"]
        },
        "clean": {
            "dependsOn": []
        },
        "check": {
            "dependsOn": ["^package"]
        },
        "dev": {
            "dependsOn": ["^package"]
        },
        "package": {
            "dependsOn": [],
            "outputs": ["./package/**"]
        }
    }
}
