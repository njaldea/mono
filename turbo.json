{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "cache": true,
            "outputs": ["./package/**"]
        },
        "clean": {},
        "check": {
            "dependsOn": ["build"]
        },
        "dev": {
            "dependsOn": ["^build"]
        },
        "vercel": {
            "dependsOn": ["^build"]
        }
    }
}
